package com.jlcindia.pattern;

import java.io.IOException;

public class Pattern11 {

	public static void main(String[] args) {
		System.out.println("Enter the Ending Charecter A to Z");
		char ch;
		try {
			ch = (char)System.in.read();
			for(char i='A';i<=ch;i++){
				
				for(char j='A';j<=i;j++){
					System.out.print(i);
				}
				System.out.println();
			}
		} catch (IOException e) {
			e.printStackTrace();
		}

	}

}
