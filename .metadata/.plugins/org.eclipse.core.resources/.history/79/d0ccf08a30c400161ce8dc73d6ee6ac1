package com.jlcindia.pattern;

import java.util.Scanner;

public class PatternTest {

	public static void main(String[] args) {
		/*System.out.println("Enter the Height of the pattern");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();*/
		
		for(int i=1;i>= 4;i++){
			
			for(int j=1;j>=i;j++){
				if(j%2 != 0)
					System.out.print(i+" ");
				else
					System.out.print(j+"*");
			}
			System.out.println();
			/*for(int k=3;k>=i;k--){
				System.out.print();
			}
			*/
			System.out.println();
		}

	}

}
