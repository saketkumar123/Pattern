package com.jlcindia.pattern;

import java.util.Scanner;

public class Pattern53 {

	public static void main(String[] args) {
		System.out.println("Enter the Height of the pattern");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int z = 1;
		for(int i=1;i<= n;i++){
			
			for(int j=1;j<=i;j++){
				System.out.print("*");
			}
			System.out.println();
		}

	}

}
