		try {
			System.out.println("Enter the Starting Charecter A to Z");
			ch1 = (char)System.in.read();
			System.out.println("Enter the Ending Charecter A to Z");
			char ch2 = (char)System.in.read();
			/*for(char i=ch1;i<=ch2;i++){
				
				for(char j=ch1;j<=i;j++){
					System.out.print(i);
				}
				System.out.println();
			}*/
		} catch (IOException e) {
			e.printStackTrace();
		}

	}

}
