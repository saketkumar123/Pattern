package com.jlcindia.pattern;

import java.util.Scanner;

public class Pattern65 {

	public static void main(String[] args) {
		/*System.out.println("Enter the Height of the pattern");
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();*/
		
		for(int i=1;i<=5;i++){
			
			for(int k=4;k>=i;k--){
				System.out.print("  ");
			}
			
			for(int j=1;j<=i;j++){
				System.out.print(i+"  ");
			}
			
			System.out.println();
		}

	}

}
